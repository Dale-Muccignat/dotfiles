Host *
	ServerAliveInterval 15
	ServerAliveCountMax 20
	TCPKeepAlive no
	ControlPath ~/.ssh/master-%r@%h:%p
	ControlMaster auto
	ControlPersist 15m
	Compression yes
	VisualHostKey yes
	SendEnv LANG
    AddKeysToAgent yes

Host hpc
	user jc321258
	Hostname zodiac.hpc.jcu.edu.au
	Port 8822
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa
    SetEnv PATH=/bin:/usr/bin:/usr/local/bin

